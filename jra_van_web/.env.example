# Google Cloud project ID used for BigQuery access.
GOOGLE_CLOUD_PROJECT=your-gcp-project-id

# BigQuery dataset that provides serving tables for the web app.
JRA_SERVING_DATASET=jra_serving

# BigQuery dataset that provides historical analysis view.
JRA_ANALYSIS_DATASET=jra_common

# BigQuery location used by query jobs.
BIGQUERY_LOCATION=asia-northeast1

# Absolute path to a service-account JSON key file stored outside this repository.
# Leave unset if your environment uses Application Default Credentials (ADC).
GOOGLE_APPLICATION_CREDENTIALS=C:\\secure\\path\\service-account.json
